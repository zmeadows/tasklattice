{
  "typeCheckingMode": "standard",
  "reportUnreachable": "none",
  "executionEnvironments": [
    { "root": ".", "pythonVersion": "3.11" }
  ],
  "include": ["src", "tests"],
  "exclude": [
    "**/.venv",
    "**/venv",
    "**/env",
    "**/__pycache__",
    "**/site-packages/**",
    "build",
    "dist"
  ],
}

